WAP to generate all combinations of 1, 2 and 3

#include<stdio.h>

int main(){

    int arr[] = {1,2,3};

    for(int i=0; i<3; i++){
        for(int j=0; j<3; j++){
            for(int k=0; k<3; k++){
                printf("%d%d%d\n", arr[i],arr[j],arr[k]);
            }
        }
    }

return 0;
}
