WAP to generate divisors of an integer.

#include<stdio.h>

int main(){

    int num;
    printf("Enter a number:");
    scanf("%d", &num);

    printf("Divisors of the given number are:\n");
    for(int i=1; i<=num/2; i++){
        if(num%i==0){
            printf("%d\n", i);
        }
    }
    printf("%d\n", num);

return 0;
}
