 WAP to find out the largest of three numbers. 

#include <stdio.h>

int main() {

    int a,b,c;
    printf("Enter 3 numbers:\n");
    scanf("%d%d%d", &a, &b, &c);

    if(a>=b){
        if(a>=c){
            printf("%d is Largest", a);
        }
        else{
            printf("%d is Largest", c);
        }
    }

    else if(b>=c){
        printf("%d is Largest", b);
    }
    else{
        printf("%d is Largest", c);
    }


    return 0;
}

